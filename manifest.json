{
  "manifest_version": 3,
  "name": "Workspace Manager",
  "version": "1.0",
  "description": "Manage workspaces and tabs with to-do lists.",
  "permissions": ["storage", "tabs", "activeTab", "windows"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
